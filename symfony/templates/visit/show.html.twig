{% extends 'admin/layout.html.twig' %}

{% block title %}Visit{% endblock %}

{% block body_content %}
    <h1>Visit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visit.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ visit.type }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ visit.ip }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ visit.createdAt ? visit.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Agent</th>
                <td>{{ visit.agent }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('visit_index') }}">back to list</a>

    <a href="{{ path('visit_edit', {'id': visit.id}) }}">edit</a>

    {{ include('visit/_delete_form.html.twig') }}
{% endblock %}
